<div class="delete">
  <form id="deleteRank" method="POST" href="/ranks/{{this.rank.rankID}}/delete">
      <legend><h3>Delete Rank</h3></legend>
          <fieldset class="fields">
            <p>Are you sure you wish to delete Rank ID: {{this.rank.rankID}}?</p>
            <div class="field">
                <label> Rank Name </label> 
                <input type="text" name="rankName" value="{{this.rank.rankName}}" disabled>
            </div>
    </fieldset>
    <input class="btn" type="submit" id="DeleteRank" value="Delete Rank">
    <a class="btn" href="/ranks"><input class="btn" type="button" value="Cancel"></a>
  </form> 
</div><!-- delete -->